
<div class="col-md-3">
    <article class="card">
        <img class="card-img-top" src="{{image_url}}" alt="{{name}}">
        <div class="card-body">
            <h5 class="card-title">{{name}} </h5>
            <p class="card-text"> {{description}}</p>
            <p class="card-text"></p>{{tagline}}</p>
        </div>
    </article>
</div>